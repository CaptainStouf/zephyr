{
  "extends": ["tslint-config-standard"],
  "env": {
    "node": true
  },
  "rules": {
    "indent": [true, "spaces"],
    "ter-indent": [true, 2],
    "space-before-function-paren": ["error", {
      "anonymous": "always",
      "named": "never",
      "asyncArrow": "ignore"
    }],
    "semicolon": [true, "always"],
    "no-unused-variable": null
  }
}
